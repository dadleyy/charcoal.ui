  },
  "author": "Danny Hadley <dadleyy@gmail.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/dadleyy/charcoal.ui/issues"
  },
  "config": {
    "build": {
      "base": "./",
      "dist": {
        "app": "./dist/app",
        "docs": "./dist/docs"
      }
    }
  },
  "homepage": "https://github.com/dadleyy/charcoal.ui#readme",
