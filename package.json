{
  "name": "charcoal-ui",
  "version": "0.0.1",
  "description": "the browser-side application for charcoal",
  "scripts": {
    "build": "gulp",
    "build:debug": "gulp",
    "build:watch": "gulp watch",
    "build:release": "gulp release",
    "build:docs": "gulp js:docs",
    "build:watch:docs": "gulp watch:docs",
    "clean": "gulp clean",
    "vendor:install": "bower i && typings i",
    "test": "npm run test:unit",
    "test:unit": "karma start --single-run=true --browsers=PhantomJS",
    "test:watch": "karma start --single-run=false --browsers=PhantomJS",
    "test:browser": "karma start --single-run=false --browsers=Chrome",
    "start": "./bin/server"
  },
  "repository": {
    "type": "git",
